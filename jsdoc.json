
{
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
      "include": ["src", "README.md"],
      "includePattern": ".+\\.(js|jsx|ts|tsx)$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
      "plugins/markdown",
      "jsdoc-ts-utils",
      "node_modules/better-docs/typescript"
    ],
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
    },
    "opts": {
      "encoding": "utf8",
      "destination": "docs/",
      "recurse": true,
      "verbose": true,
        "template": "node_modules/better-docs"
      }
  }
  